{
  "ecmaFeatures": {
    "modules": true
  },
  "plugins": [
    "import",
    "jasmine"
  ],
  "env": {
    "es6": true,
    "jasmine": true,
    "node": true
  },
  "rules": {
    "comma-dangle": [0],
    "max-len": [1, {
      "code": 200
    }],
    "no-param-reassign": [2, {
      "props": false
    }],
    "no-console": [0],
    "no-new-func": [0],
    "no-plusplus": [2, {
      "allowForLoopAfterthoughts": true
    }],
    "prefer-rest-params": [0],
    "jasmine/no-spec-dupes": [0],
    "valid-jsdoc": [2, {
      "requireReturnType": false
    }]
  },
  "extends": [
    "airbnb",
    "plugin:jasmine/recommended"
  ],
  "parser": "babel-eslint"
}
